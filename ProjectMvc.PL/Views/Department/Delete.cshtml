@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete @Model.Name Department</h1>
<br />
<form asp-action="Delete">
    <div class="form-group">
        <label class="form-label" asp-for="Code"></label>
        <input readonly class="form-control" asp-for="Code" />
    </div>

    <br />

    <div class="form-group">
        <label class="form-label" asp-for="Name"></label>
        <input readonly class="form-control" asp-for="Name" />
    </div>

    <br />

    <div class="form-group">
        <label class="form-label" asp-for="DateOfCreation"></label>
        <input readonly class="form-control" asp-for="DateOfCreation" />
    </div>

    <br />
    <br />
    <center>
        <input type="submit" value="Delete" class="btn btn-danger" />

        <a asp-action="Index" class="btn btn-outline-secondary">Cansle</a>
    </center>
</form>
